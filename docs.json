{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "LaunchMC",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
